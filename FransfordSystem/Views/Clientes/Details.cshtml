@model FransfordSystem.Models.Cliente

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles del cliente</h1>

<div>
    <h4>Consulta de la información completa del cliente</h4>
    <hr />
    <dl class="row">  
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.dui)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dui)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.nombreCliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nombreCliente)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.apellidoCliente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.apellidoCliente)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.fechaNacimiento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.fechaNacimiento)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.genero)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.genero)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.telefono)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.telefono)
        </dd>
        
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.IdCliente">Editar</a> |
    <a asp-action="Index">Regresar a la lista de clientes</a>
</div>
